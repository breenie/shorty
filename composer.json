{
    "name": "kurl/shorty",
    "description": "Another URL shortener",
    "license": "MIT",
    "version": "0.1.0",
    "type": "project",
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "~1.2",
        "silex/web-profiler": "~1.0",
        "symfony/browser-kit": "~2.3",
        "symfony/class-loader": "~2.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/css-selector": "~2.3",
        "symfony/debug": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/form": "~2.3",
        "symfony/monolog-bridge": "~2.3",
        "monolog/monolog": "~1.12",
        "symfony/process": "~2.3",
        "symfony/security": "~2.3",
        "symfony/translation": "~2.3",
        "symfony/twig-bridge": "~2.3",
        "symfony/validator": "~2.3",
        "doctrine/dbal": "2.2.*",
        "doctrine/migrations": "1.0.*@dev",
        "kurl/base62": "~1.0",
        "kurl/silex-doctrine-migrations-provider": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "heroku/heroku-buildpack-php": "*"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "autoload-dev" :{
        "psr-0": { "": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        },
        "heroku": {
            "framework": "silex",
            "document-root": "web",
            "index-document": "index.php"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "post-install-cmd": [
        ]
    },
    "bin": [
        "bin/console"
    ]
}
