{
    "name": "kurl/shorty",
    "description": "Another URL shortener",
    "license": "MIT",
    "version": "0.1.0",
    "type": "project",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ironsummitmedia/startbootstrap-freelancer",
                "version": "1.0.0",
                "dist": {
                    "url": "https://github.com/IronSummitMedia/startbootstrap-freelancer/archive/v1.0.0.zip",
                    "type": "zip"
                },
                "source": {
                    "type": "git",
                    "url": "git@github.com:IronSummitMedia/startbootstrap-freelancer.git",
                    "reference": "tags/v1.0.0"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ironsummitmedia/startbootstrap-freelancer": "~1.0",
        "silex/silex": "~1.0",
        "silex/web-profiler": "~1.0",
        "symfony/browser-kit": "~2.3",
        "symfony/class-loader": "~2.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/css-selector": "~2.3",
        "symfony/debug": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/form": "~2.3",
        "symfony/monolog-bridge": "~2.3",
        "symfony/process": "~2.3",
        "symfony/security": "~2.3",
        "symfony/translation": "~2.3",
        "symfony/twig-bridge": "~2.3",
        "symfony/validator": "~2.3",
        "doctrine/dbal": "2.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "heroku/heroku-buildpack-php": "*"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "post-install-cmd": [
            "cp -af vendor/ironsummitmedia/startbootstrap-freelancer/css web/css",
            "cp -af vendor/ironsummitmedia/startbootstrap-freelancer/js web/js",
            "cp -af vendor/ironsummitmedia/startbootstrap-freelancer/font-awesome-4.1.0 web/font-awesome-4.1.0",
            "cp -af vendor/ironsummitmedia/startbootstrap-freelancer/fonts web/fonts"
        ],
        "post-update-cmd": [
            "cp -af vendor/ironsummitmedia/startbootstrap-freelancer/css web/css",
            "cp -af vendor/ironsummitmedia/startbootstrap-freelancer/js web/js",
            "cp -af vendor/ironsummitmedia/startbootstrap-freelancer/font-awesome-4.1.0 web/font-awesome-4.1.0",
            "cp -af vendor/ironsummitmedia/startbootstrap-freelancer/fonts web/fonts"
        ]
    }
}
